<GlueSettings>

    <!-- different kinds of glue used in the glue table, values set in mu (math units); 1 mu = 1/18 em -->

    <GlueTypes>
        <GlueType name="default" shrink="0" space="0" stretch="0" />
        <GlueType name="thin" shrink="0" space="3" stretch="0" />
        <GlueType name="med" shrink="4" space="4" stretch="2" />
        <GlueType name="thick" shrink="0" space="5" stretch="5" />
    </GlueTypes>

    <!-- Glue table for the 4 possible TeX-styles (display, text, script and script_script) and the 8 possible
    atom types (rd, op, bin, rel, open, close, punct and inner). Other atom types (over, under, acc, rad and vcent)
    are treated as type ord. No table entry for a specific lefttype, righttype and style means gluetype "default"
    Page 181 in TeXBook
    -->

    <GlueTable>
        <Glue gluetype="thin" lefttype="ord" righttype="op">
            <Style name="display" />
            <Style name="text" />
            <Style name="script" />
            <Style name="script_script" />
        </Glue>
        <Glue gluetype="med" lefttype="ord" righttype="bin">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="ord" righttype="rel">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="ord" righttype="inner">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="op" righttype="ord">
            <Style name="display" />
            <Style name="text" />
            <Style name="script" />
            <Style name="script_script" />
        </Glue>
        <Glue gluetype="thin" lefttype="op" righttype="op">
            <Style name="display" />
            <Style name="text" />
            <Style name="script" />
            <Style name="script_script" />
        </Glue>
        <Glue gluetype="thick" lefttype="op" righttype="rel">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="op" righttype="inner">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="med" lefttype="bin" righttype="ord">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="med" lefttype="bin" righttype="op">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="med" lefttype="bin" righttype="open">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="med" lefttype="bin" righttype="inner">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="rel" righttype="ord">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="rel" righttype="op">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="rel" righttype="open">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="rel" righttype="inner">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="close" righttype="op">
            <Style name="display" />
            <Style name="text" />
            <Style name="script" />
            <Style name="script_script" />
        </Glue>
        <Glue gluetype="med" lefttype="close" righttype="bin">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="close" righttype="rel">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="close" righttype="inner">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="punct" righttype="ord">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="med" lefttype="punct" righttype="op">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="punct" righttype="rel">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="punct" righttype="open">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="punct" righttype="close">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="med" lefttype="punct" righttype="punct">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="punct" righttype="inner">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="inner" righttype="ord">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="inner" righttype="op">
            <Style name="display" />
            <Style name="text" />
            <Style name="script" />
            <Style name="script_script" />
        </Glue>
        <Glue gluetype="med" lefttype="inner" righttype="bin">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thick" lefttype="inner" righttype="rel">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="inner" righttype="open">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="inner" righttype="punct">
            <Style name="display" />
            <Style name="text" />
        </Glue>
        <Glue gluetype="thin" lefttype="inner" righttype="inner">
            <Style name="display" />
            <Style name="text" />
        </Glue>
    </GlueTable>
</GlueSettings>
